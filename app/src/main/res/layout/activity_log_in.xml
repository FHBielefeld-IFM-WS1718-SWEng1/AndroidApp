<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="partyplaner.partyplaner.LogIn.LogInActivity">

    <fragment
        android:id="@+id/fragment3"
        android:name="partyplaner.partyplaner.LogIn.LogoFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_login_logo" />

    <fragment
        android:id="@+id/fragment4"
        android:name="partyplaner.partyplaner.LogIn.SignInFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_login_signin" />
</LinearLayout>
